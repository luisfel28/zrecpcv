{
  "_version": "1.28.0",
  "sap.app": {
    "id": "fidelidademundial.com.zrecpcv",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.4",
      "toolsId": "a18af403-9dba-41d2-93fd-b80e6843bc86"
    },
    "dataSources": {
      "ZRE_GW_CPCV_ANNO_MDL": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZRE_GW_CPCV_ANNO_MDL',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZRE_GW_CPCV_ANNO_MDL.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZRE_GW_CPCV_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZRE_GW_CPCV_ANNO_MDL"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.84.7",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fidelidademundial.com.zrecpcv.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "main": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }      
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "fidelidademundial.com.zrecpcv.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Routecontratos",
          "pattern": ":?query:",
          "target": [
            "Targetcontratos"
          ]
        },
        {
          "name": "Routecriacao",
          "pattern": "criacao/{bukrs}/{recnnr}/{partner}",
          "target": "TargetCriacao"
        }  
      ],
      "targets": {
        "Targetcontratos": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "contratos",
          "viewName": "contratos"
        },
        "TargetCriacao": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "criacao",
          "viewName": "criacao",
          "viewLevel": 2
        }     
      }
    },
    "rootView": {
      "viewName": "fidelidademundial.com.zrecpcv.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
